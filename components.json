{
  "$schema": "https://ui.shadcn.com/schema.json",
  "style": "new-york",
  "rsc": true,
  "tsx": true,
  "tailwind": {
    "config": "tailwind.config.js",
    "css": "src/app/globals.css",
    "baseColor": "neutral",
    "cssVariables": true,
    "prefix": ""
  },
  "iconLibrary": "lucide",
  "aliases": {
    "components": "@/components",
    "utils": "@/lib/utils",
    "ui": "@/components/ui",
    "lib": "@/lib",
    "hooks": "@/hooks"
  },
  "registries": {
    "@acertenity": {
      "name": "Acertenity",
      "url": "https://ui.aceternity.com/registry/{name}.json"
    },
    "@originui": {
      "name": "Origin UI",
      "url": "https://originui.com/r/{name}.json"
    },
    "@cult-ui": {
      "name": "Cult UI",
      "url": "https://cult-ui.com/r/{name}.json"
    },
    "@kibo-ui": {
      "name": "Kibo UI",
      "url": "https://kibo-ui.com/r/{name}.json"
    },
    "@reui": {
      "name": "ReUI",
      "url": "https://reui.io/r/{name}.json"
    }
  }
}
